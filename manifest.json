{
    "manifest_version": 2,
    "name": "SciFi Launcher",
    "description": "Simple Firefox addon to show 12 most visited websites",
    "icons": {
        "32": "icons/logo_32.png",
        "64": "icons/logo_64.png",
        "128": "icons/logo_128.png"
    },
    "version": "1.0",
    "chrome_url_overrides": {
        "newtab": "sites.html"
    },
    "permissions": [
        "topSites"
    ],
    "browser_specific_settings": {
        "gecko": {
            "strict_min_version": "54.0a1"
        }
    }
}
